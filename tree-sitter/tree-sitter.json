{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "isograph",
      "camelcase": "Isograph",
      "title": "Isograph",
      "scope": "source.isograph",
      "file-types": ["isograph"],
      "injection-regex": "^isograph$",
      "class-name": "TreeSitterIsograph"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Isograph grammar for tree-sitter",
    "authors": [
      {
        "name": "Isograph Labs"
      }
    ],
    "links": {
      "repository": "https://github.com/isographlabs/isograph"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
