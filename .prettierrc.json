{
  "importOrderTypeScriptVersion": "5.6.3",
  "overrides": [
    {
      "files": ["*.json"],
      "options": {
        "trailingComma": "none"
      }
    }
  ],
  "plugins": ["@ianvs/prettier-plugin-sort-imports", "@prettier/plugin-oxc"],
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all"
}
