{
  "name": "@isograph/react",
  "version": "0.5.3",
  "description": "Use Isograph with React",
  "homepage": "https://isograph.dev",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.cts",
  "author": "Isograph Labs",
  "license": "MIT",
  "scripts": {
    "compile-libs": "tsdown",
    "watch-libs": "tsdown --watch",
    "test": "vitest run",
    "test-watch": "vitest watch",
    "coverage": "vitest run --coverage",
    "prepack": "pnpm run test && pnpm run compile-libs",
    "tsc": "tsc -b",
    "tsc-force": "tsc --build --clean && tsc --build --force",
    "iso": "../../target/debug/isograph_cli --config ./isograph.config.json"
  },
  "dependencies": {
    "@isograph/disposable-types": "workspace:*",
    "@isograph/react-disposable-state": "workspace:*",
    "@isograph/reference-counted-pointer": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.24.7",
    "@types/react": "18.3.1",
    "react-test-renderer": "^18.2.0",
    "typescript": "5.6.3",
    "vite-plugin-babel": "^1.2.0",
    "vite-plugin-commonjs": "^0.10.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/isographlabs/isograph.git",
    "directory": "libs/isograph-react"
  },
  "sideEffects": false,
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  }
}
